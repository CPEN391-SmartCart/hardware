
#include "digital.h"

int digitalRead(volatile unsigned int *address)
{
	return *address;
}

void digitalWrite(volatile unsigned int *address, int value)
{
	*address = value;
}

uint8_t shiftInMSB(uint8_t dataPin, uint8_t clockPin) {

 uint8_t value = 0;

 uint8_t i;

 for (i = 0; i < 8; ++i) {

 digitalWrite(clockPin, HIGH);

 if (bitOrder == LSBFIRST)

 value |= digitalRead(dataPin) << i;

 else

 value |= digitalRead(dataPin) << (7 - i);

 digitalWrite(clockPin, LOW);

 }

 return value;

}
